import java.util.Scanner;

public class Table
{
    public static void main()
    {
        int i,n;
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter a no. whose table is to be written");
        n=sc.nextInt();
        for(i=1;i<=10;i++)
        {
            System.out.println(n+" * "+i+" = "+(n*i));
        }
    }
}
